{
  "name": "pyoh-website",
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/images/.+",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/robots.txt",
      "headers": {
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "src": "/sitemap.xml",
      "headers": {
        "cache-control": "public, max-age=86400"
      }
    }
  ],
  "env": {
    "NUXT_PUBLIC_STRAPI_URL": "@nuxt_public_strapi_url",
    "NUXT_PUBLIC_STRAPI_TOKEN": "@nuxt_public_strapi_token",
    "NUXT_PUBLIC_SITE_NAME": "@nuxt_public_site_name",
    "NUXT_PUBLIC_SITE_DESCRIPTION": "@nuxt_public_site_description",
    "NUXT_PUBLIC_SITE_URL": "@nuxt_public_site_url",
    "NUXT_PUBLIC_MEDIA_URL": "@nuxt_public_media_url",
    "NUXT_PUBLIC_ENABLE_ANIMATIONS": "@nuxt_public_enable_animations",
    "NUXT_PUBLIC_ANIMATION_DURATION": "@nuxt_public_animation_duration",
    "NUXT_PUBLIC_PROJECTS_PER_PAGE": "@nuxt_public_projects_per_page",
    "NUXT_PUBLIC_FEATURED_PROJECTS_LIMIT": "@nuxt_public_featured_projects_limit",
    "NUXT_PUBLIC_DEFAULT_CATEGORIES": "@nuxt_public_default_categories",
    "NUXT_PUBLIC_DEFAULT_TECHNOLOGIES": "@nuxt_public_default_technologies",
    "NUXT_PUBLIC_META_TITLE": "@nuxt_public_meta_title",
    "NUXT_PUBLIC_META_DESCRIPTION": "@nuxt_public_meta_description",
    "NUXT_PUBLIC_META_KEYWORDS": "@nuxt_public_meta_keywords",
    "NUXT_PUBLIC_META_AUTHOR": "@nuxt_public_meta_author",
    "NUXT_PUBLIC_META_IMAGE": "@nuxt_public_meta_image",
    "NUXT_PUBLIC_SOCIAL_LINKEDIN": "@nuxt_public_social_linkedin",
    "NUXT_PUBLIC_SOCIAL_TWITTER": "@nuxt_public_social_twitter",
    "NUXT_PUBLIC_SOCIAL_INSTAGRAM": "@nuxt_public_social_instagram",
    "NUXT_PUBLIC_SOCIAL_BEHANCE": "@nuxt_public_social_behance",
    "NUXT_PUBLIC_CONTACT_EMAIL": "@nuxt_public_contact_email",
    "NUXT_PUBLIC_CONTACT_PHONE": "@nuxt_public_contact_phone",
    "NUXT_PUBLIC_CONTACT_ADDRESS": "@nuxt_public_contact_address",
    "NUXT_PUBLIC_SERVICES_WEB_DESIGN": "@nuxt_public_services_web_design",
    "NUXT_PUBLIC_SERVICES_BRANDING": "@nuxt_public_services_branding",
    "NUXT_PUBLIC_SERVICES_MOBILE_APP": "@nuxt_public_services_mobile_app",
    "NUXT_PUBLIC_SERVICES_UX_UI": "@nuxt_public_services_ux_ui",
    "NUXT_PUBLIC_SERVICES_PRINT": "@nuxt_public_services_print",
    "NUXT_PUBLIC_ENABLE_LAZY_LOADING": "@nuxt_public_enable_lazy_loading",
    "NUXT_PUBLIC_ENABLE_IMAGE_OPTIMIZATION": "@nuxt_public_enable_image_optimization",
    "NUXT_PUBLIC_ENABLE_CACHING": "@nuxt_public_enable_caching",
    "NUXT_PUBLIC_ENABLE_DEBUG": "@nuxt_public_enable_debug",
    "NUXT_PUBLIC_ENABLE_HOT_RELOAD": "@nuxt_public_enable_hot_reload",
    "NUXT_PUBLIC_ENABLE_DEVTOOLS": "@nuxt_public_enable_devtools",
    "NUXT_PUBLIC_ENABLE_ANALYTICS": "@nuxt_public_enable_analytics",
    "NUXT_PUBLIC_ENABLE_ERROR_TRACKING": "@nuxt_public_enable_error_tracking",
    "NUXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING": "@nuxt_public_enable_performance_monitoring",
    "NUXT_PUBLIC_ENABLE_TESTING": "@nuxt_public_enable_testing",
    "NUXT_PUBLIC_TEST_API_URL": "@nuxt_public_test_api_url",
    "NUXT_PUBLIC_DEPLOYMENT_URL": "@nuxt_public_deployment_url",
    "NUXT_PUBLIC_DEPLOYMENT_ENVIRONMENT": "@nuxt_public_deployment_environment"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "regions": ["cdg1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  }
}
