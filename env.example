# Configuration Strapi
NUXT_PUBLIC_STRAPI_URL=http://localhost:1337
NUXT_PUBLIC_STRAPI_TOKEN=your_strapi_api_token_here

# Configuration de l'environnement
NODE_ENV=development

# Configuration du site
NUXT_PUBLIC_SITE_NAME=PYOH
NUXT_PUBLIC_SITE_DESCRIPTION=Agence créative spécialisée dans la conception de sites web, l'identité visuelle et l'expérience utilisateur
NUXT_PUBLIC_SITE_URL=http://localhost:3000

# Configuration des médias
NUXT_PUBLIC_MEDIA_URL=http://localhost:1337

# Configuration des animations (optionnel)
NUXT_PUBLIC_ENABLE_ANIMATIONS=true
NUXT_PUBLIC_ANIMATION_DURATION=600

# Configuration du portfolio
NUXT_PUBLIC_PROJECTS_PER_PAGE=12
NUXT_PUBLIC_FEATURED_PROJECTS_LIMIT=6

# Configuration des catégories par défaut
NUXT_PUBLIC_DEFAULT_CATEGORIES=Web Design,Branding,Mobile App,UI/UX,Print

# Configuration des technologies par défaut
NUXT_PUBLIC_DEFAULT_TECHNOLOGIES=Vue.js,React,Node.js,TypeScript,Tailwind CSS,GSAP

# Configuration des métadonnées
NUXT_PUBLIC_META_TITLE=PYOH - Agence Créative
NUXT_PUBLIC_META_DESCRIPTION=Agence créative spécialisée dans la conception de sites web, l'identité visuelle et l'expérience utilisateur
NUXT_PUBLIC_META_KEYWORDS=agence créative,web design,branding,ux ui,site web,logo,identité visuelle
NUXT_PUBLIC_META_AUTHOR=PYOH
NUXT_PUBLIC_META_IMAGE=/images/og-image.jpg

# Configuration des réseaux sociaux
NUXT_PUBLIC_SOCIAL_LINKEDIN=https://linkedin.com/company/pyoh
NUXT_PUBLIC_SOCIAL_TWITTER=https://twitter.com/pyoh_agency
NUXT_PUBLIC_SOCIAL_INSTAGRAM=https://instagram.com/pyoh_agency
NUXT_PUBLIC_SOCIAL_BEHANCE=https://behance.net/pyoh

# Configuration des contacts
NUXT_PUBLIC_CONTACT_EMAIL=contact@pyoh.com
NUXT_PUBLIC_CONTACT_PHONE=+33 1 23 45 67 89
NUXT_PUBLIC_CONTACT_ADDRESS=123 Rue de la Créativité, 75001 Paris, France

# Configuration des services
NUXT_PUBLIC_SERVICES_WEB_DESIGN=true
NUXT_PUBLIC_SERVICES_BRANDING=true
NUXT_PUBLIC_SERVICES_MOBILE_APP=true
NUXT_PUBLIC_SERVICES_UX_UI=true
NUXT_PUBLIC_SERVICES_PRINT=true

# Configuration des performances
NUXT_PUBLIC_ENABLE_LAZY_LOADING=true
NUXT_PUBLIC_ENABLE_IMAGE_OPTIMIZATION=true
NUXT_PUBLIC_ENABLE_CACHING=true

# Configuration du développement
NUXT_PUBLIC_ENABLE_DEBUG=false
NUXT_PUBLIC_ENABLE_HOT_RELOAD=true
NUXT_PUBLIC_ENABLE_DEVTOOLS=true

# Configuration de la production
NUXT_PUBLIC_ENABLE_ANALYTICS=false
NUXT_PUBLIC_ENABLE_ERROR_TRACKING=false
NUXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=false

# Configuration des tests
NUXT_PUBLIC_ENABLE_TESTING=false
NUXT_PUBLIC_TEST_API_URL=http://localhost:1337

# Configuration du déploiement
NUXT_PUBLIC_DEPLOYMENT_URL=https://pyoh.com
NUXT_PUBLIC_DEPLOYMENT_ENVIRONMENT=production

